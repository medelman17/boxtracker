{
  "name": "@boxtrack/ui",
  "version": "0.1.0",
  "private": true,
  "description": "Shared UI components for BoxTrack",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external react",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "dependencies": {
    "@boxtrack/shared": "workspace:*",
    "react-qr-code": "^2.0.18"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "eslint": "^9.0.0",
    "react": "^19.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.8.0"
  }
}
